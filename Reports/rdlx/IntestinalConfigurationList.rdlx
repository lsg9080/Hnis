	<Code>Function GetPreType(ByVal u As Integer) As String
	If(u &gt; 0) Then
		If(u = 1) Then
			Return "水剂"
		Else If(u = 2) Then
			Return "粉剂"
		End If
	Else
		Return "--"
	End If
End Function</Code>
