	<DataSources>
		<DataSource Name="HNTS">
			<ConnectionProperties>
				<ConnectString>user id=sa;password=qwe123!@#;data source=192.172.2.154;initial catalog=HNTS;persist security info=True;</ConnectString>
				<DataProvider>SQL</DataProvider>
			</ConnectionProperties>
			<DataSourceReference>../..\RDSX\hnts.rdsx</DataSourceReference>
			<Transaction>true</Transaction>
		</DataSource>
	</DataSources>
	<DataSets>
		<DataSet Name="YYSDDataSet">
			<Fields>
				<Field Name="AccommodationID">
					<DataField>AccommodationID</DataField>
				</Field>
				<Field Name="PatientCode">
					<DataField>PatientCode</DataField>
				</Field>
				<Field Name="AID">
					<DataField>AID</DataField>
				</Field>
				<Field Name="DayNum">
					<DataField>DayNum</DataField>
				</Field>
				<Field Name="MealTtimes">
					<DataField>MealTtimes</DataField>
				</Field>
				<Field Name="MenuName">
					<DataField>MenuName</DataField>
				</Field>
				<Field Name="FoodName">
					<DataField>FoodName</DataField>
				</Field>
				<Field Name="Num">
					<DataField>Num</DataField>
				</Field>
				<Field Name="Unit">
					<DataField>Unit</DataField>
				</Field>
			</Fields>
			<Query>
				<CommandText>select sa.AccommodationID, sa.PatientCode, am.AID,
af.DayNum,af.MealTtimes,af.MenuName,afm.FoodName,afm.Num,
case LEN(Unit) when 0 then isnull(PortionUnit,'g') else isnull(Unit,'g') end Unit from dbo.T_AccommodationFoodMenu  afm 
left join  dbo.T_AccommodationFood af on af.AccommodationFoodID = afm.AccommodationFoodID
left join dbo.T_Accommodation sa on sa.AccommodationID = af.AccommodationID and sa.AccommodationType = 4 
left join S_FoodDetails fd on fd.FoodCode=afm.FoodCode 
left join dbo.T_Accomm am on charindex(',' +LTRIM(sa.AccommodationID), ','+am.AccommodationIDs) &gt; 0
where am.AID = @aid and sa.AccommodationID = @accommodationID order by af.DayNum,af.MealTtimesID</CommandText>
				<DataSourceName>HNTS</DataSourceName>
				<QueryParameters>
					<QueryParameter Name="@aid">
						<Value>=Parameters!aid.Value</Value>
					</QueryParameter>
					<QueryParameter Name="accommodationID">
						<Value>=Parameters!accommodationID.Value</Value>
					</QueryParameter>
				</QueryParameters>
			</Query>
		</DataSet>
	</DataSets>
