					<DataField>Sex</DataField>
				</Field>
				<Field Name="AgeStr">
					<DataField>AgeStr</DataField>
				</Field>
				<Field Name="FName">
					<DataField>FName</DataField>
				</Field>
				<Field Name="BeInNo">
					<DataField>BeInNo</DataField>
				</Field>
				<Field Name="In_Type">
					<DataField>In_Type</DataField>
				</Field>
				<Field Name="AdviceType">
					<DataField>AdviceType</DataField>
				</Field>
				<Field Name="EN_Num">
					<DataField>EN_Num</DataField>
				</Field>
				<Field Name="In_Date">
					<DataField>In_Date</DataField>
				</Field>
				<Field Name="IsFinished">
					<DataField>IsFinished</DataField>
				</Field>
				<Field Name="Price">
					<DataField>Price</DataField>
				</Field>
				<Field Name="FullName">
					<DataField>FullName</DataField>
				</Field>
				<Field Name="CCID">
					<DataField>CCID</DataField>
				</Field>
				<Field Name="HTML">
					<DataField>HTML</DataField>
				</Field>
			</Fields>
			<Query>
				<CommandText>select
In_ID,
tm.PrescriptionContent, 
tm.PatientCode, --ID
isnull(tp.PatientName,'--') as PatientName,
(case tp.Sex when 0 then '女' when 1 then '男' else '--' end) as Sex,
tp.AgeStr,
isnull(sp.FName,'') as FName, --用法
isnull(tp.BeInNo,'') as BeInNo, --住院号
--(case when EN_Amount &gt; 0 then ltrim(cast(EN_Amount as float)) +  'ml/次' else '--' end) as EN_Amount,
--(case when EN_Temperature &gt; 0 then ltrim(EN_Temperature) +  '℃' else '--' end) as EN_Temperature,
--(case when EN_Speed &gt; 0 then ltrim(EN_Speed) +  'ml/h' else '--' end) as EN_Speed,
In_Type, --缴费
AdviceType,--配制
EN_Num, --天数
In_Date, --日期
--isnull(ss.ItemText,'--') as EN_Way,  --途径
--isnull(sss.ItemText,'--') as EN_Mode,  --输注
--(case when len(ltrim(EN_Remark)) = 0 then '--' else EN_Remark end) as EN_Remark, --备注
IsFinished, 
cast(tm.Price as numeric(18, 2)) as Price,
isnull(su.FullName,'--') as FullName,
CCID, --领取
dbo.Fun_CNContentHtml(0,
			(case when EN_Amount &gt; 0 then ltrim(cast(EN_Amount as float)) +  'ml/次' else '--' end),	--液量
			(case when EN_concentration &gt; 0 then ltrim(cast(EN_concentration as float)) +  '%' else '--' end),	--浓度
			isnull(sp.FName,'--'),														--用法
			EN_Num,																		--天数
			isnull(ss.ItemText,'--'),													--途径
			isnull(sss.ItemText,'--'),													--喂养方式
			(case when EN_Speed &gt; 0 then ltrim(cast(EN_Speed as float)) +  'ml/h' else '--' end),		--速度
			(case when EN_Temperature &gt; 0 then ltrim(cast(EN_Temperature as float)) +  '℃' else '--' end),--温度
			(case when len(ltrim(EN_Remark)) = 0 then '--' else EN_Remark end),			--备注
			'none',																		--使用时间
			'none',												--剂型
			'none',														--配送
			'none',														--配方
			'none',															--保质期
			'none',
			'none'
			) as HTML
from dbo.T_MZCNIntervention tm
left join dbo.S_PreparationFrequency sp on tm.EN_UseCode = sp.FCode
left join T_Patient tp on tp.PatientCode = tm.PatientCode and tp.PatientType = 2
left join dbo.S_SelectedItem ss on ss.ItemValue = tm.EN_Way and ss.ItemType = 8
left join dbo.S_SelectedItem sss on sss.ItemValue = tm.EN_Mode and sss.ItemType = 9
left join dbo.S_UserInfo su on su.UserName = tm.CreatedBy
where tm.In_ID = @key</CommandText>
				<DataSourceName>HNTS</DataSourceName>
				<QueryParameters>
					<QueryParameter Name="@key">
						<Value>=Parameters!key.Value</Value>
					</QueryParameter>
				</QueryParameters>
			</Query>
