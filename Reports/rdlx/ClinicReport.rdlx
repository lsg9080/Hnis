					<DataField>isYYPC</DataField>
				</Field>
				<Field Name="isCNCF">
					<DataField>isCNCF</DataField>
				</Field>
				<Field Name="IsSSDLDC">
					<DataField>IsSSDLDC</DataField>
				</Field>
				<Field Name="IsJHFPC">
