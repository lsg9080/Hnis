	<Code>Function SetValue(ByVal v As Double) As String
	If(v = -2) Then
		Return ""
	Else 
		Return v 
	End If
End Function

Function GetProcessingVal(ByVal v As String) As String
	Return Mid(v,3)  
End Function


Function SetPreparationName(ByVal v As String, ByVal p As String) As String
	If(p = "CNW0000000001") Then
		Return "【食】" + v
	Else If(p = "CNW0000000002") 
		Return "【药】" + v 
	Else
		Return v
	End If
End Function

Function SetBasicName(ByVal n As String) As String
	If(n.Length &gt; 0) Then
		Dim t = split(n, "§")
		If IsArray(t) Then
			Return t(1)
		Else
			Return n
		End If
	End If
End Function

Function GetBMIRise(ByVal n As String, ByVal value As Double, ByVal ischild As Integer) As String
	Dim content As String
	content = value.ToString()
	If(n = "BMI") Then
		Dim maxValue As Double
		maxValue = iif(ischild = 1, 17.9, 23.9)
		Dim minValue As Double
			minValue = iif(ischild = 1, 13.5, 18.5)
		If(value &lt;= minValue) Then
			content = value.ToString() + " ▼"
		Else If(value &gt;= maxValue) Then
			content = value.ToString() + " ▲"
		End If
	End If
	Return content
End Function 


