		<DataSet Name="BiochemistryDataSet">
			<Fields>
				<Field Name="PDCode">
					<DataField>PDCode</DataField>
				</Field>
				<Field Name="PD_Name">
					<DataField>PD_Name</DataField>
				</Field>
				<Field Name="SampleName">
					<DataField>SampleName</DataField>
				</Field>
			</Fields>
			<Query>
				<CommandText>select PDCode,PD_Name,SampleName from dbo.H_BiochemistryTest bt inner join dbo.H_ParameterChild pc on bt.P_Code=pc.P_Code 
left join H_ParameterDirectory pd on pd.PD_Code=bt.PDCode left join H_ParameterSample ps on ps.SampleCode=bt.SampleCode  where PatientCode=@PatientCode and pc.IsAttention=1 and bt.CreatedOn&gt;=Convert(varchar(100),@StartDate,121) and bt.CreatedOn&lt;Convert(varchar(100),DATEADD(d,1,@EndDate),121) 
GROUP BY PDCode,PD_Name,SampleName
ORDER BY PD_Name</CommandText>
				<DataSourceName>HNTS</DataSourceName>
				<QueryParameters>
					<QueryParameter Name="@PatientCode">
						<Value>=Parameters!patientCode.Value</Value>
					</QueryParameter>
					<QueryParameter Name="@StartDate">
						<Value>=Parameters!startDate.Value</Value>
					</QueryParameter>
					<QueryParameter Name="@EndDate">
						<Value>=Parameters!endDate.Value</Value>
					</QueryParameter>
				</QueryParameters>
			</Query>
