				</Field>
				<Field Name="NE_ID">
					<DataField>NE_ID</DataField>
				</Field>
			</Fields>
			<Query>
				<CommandText>select tp.PatientName,tp.BeInNo,tp.BedNo,tp.Sex,
tp.Age,tp.BMI,
dbo.Fun_GetAgeName(tp.Age) as AgeStr,
tp.BeInWeight,
tp.BeInHeight,ISNULL(sd.DepartName,'--') as DepartName,
isnull(ui.FullName,'--') Name,
NE_Content,convert(varchar(16),e.CreatedOn,120) as ScreenDate,NE_ID 
from T_Education e 
left join S_UserInfo ui on e.NE_UserId=ui.UserName 
left join T_Patient tp on  e.PatientCode = tp.PatientCode 
left join S_Department sd on tp.BranchType = sd.DepartValue
where tp.PatientCode=@patientCode and e.IsDelete=0 order by e.CreatedOn</CommandText>
				<DataSourceName>EducationDataSource</DataSourceName>
				<QueryParameters>
					<QueryParameter Name="patientCode">
						<Value>=Parameters!patientCode.Value</Value>
					</QueryParameter>
				</QueryParameters>
			</Query>
