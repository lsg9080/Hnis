	<DataSets>
		<DataSet Name="YYFXDataSet">
			<Fields>
				<Field Name="NrName">
					<DataField>NrName</DataField>
				</Field>
				<Field Name="NrVal">
					<DataField>NrVal</DataField>
				</Field>
				<Field Name="TNrVal">
