				</Field>
				<Field Name="NrUnit">
					<DataField>NrUnit</DataField>
				</Field>
			</Fields>
			<Query>
				<CommandText>select   sfn.NrName,sfn.NrVal,sfn.TNrVal,sfn.NrRange,sfn.Levl,sfn.LevlStr,sni.NrUnit
	from dbo.T_AccommodationNurittion sfn 
left join dbo.T_Accommodation sa on sa.AccommodationID = sfn.AccommodationID and sa.AccommodationType = @t
left join dbo.S_NutritentItems sni on sni.NrCode = sfn.NrCode
	where sfn.PatientCode = @patCode and sfn.AccommodationID = @acid 
and IsDisplayRp=1 and sfn.IsDelete = 0 and sfn.dy  &gt; 10</CommandText>
				<DataSourceName>HNTS</DataSourceName>
				<QueryParameters>
					<QueryParameter Name="@patCode">
						<Value>=Parameters!patCode.Value</Value>
					</QueryParameter>
					<QueryParameter Name="@dy">
						<Value>=Parameters!dy.Value</Value>
					</QueryParameter>
					<QueryParameter Name="@acid">
						<Value>=Parameters!acid.Value</Value>
					</QueryParameter>
					<QueryParameter Name="@t">
						<Value>=Parameters!t.Value</Value>
					</QueryParameter>
				</QueryParameters>
			</Query>
